{
  "name": "metromac-task-management-backend",
  "version": "1.0.0",
  "description": "metromac-task-management-backend",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "lint": "eslint . --ext .js --ignore-path .eslintignore",
    "lint:fix": "eslint . --ext .js --fix --ignore-path .eslintignore",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|json)\"",
    "postinstall": "npx prisma generate --schema=src/prisma/schema.prisma && npx prisma migrate deploy --schema=src/prisma/schema.prisma && node src/prisma/seed.js",
    "seed": "npx prisma migrate deploy --schema=src/prisma/schema.prisma && npx prisma db seed --schema=src/prisma/schema.prisma"
  },
  "keywords": [],
  "author": "Sudip Mandal",
  "license": "ISC",
  "lint-staged": {
    "src/**/*.js": "npm run lint:check && npm run prettier:check"
  },
  "devDependencies": {
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "nodemon": "^3.0.0",
    "prettier": "^2.8.8"
  },
  "dependencies": {
    "@prisma/client": "^5.10.0",
    "bcrypt": "^5.1.0",
    "cloudinary": "^2.7.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.1.3",
    "exceljs": "^4.3.0",
    "express": "^4.18.2",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.1",
    "mongoose": "^7.2.2",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.10",
    "papaparse": "^5.4.1",
    "prisma": "^5.10.0",
    "rc-tree": "^5.13.1",
    "uuid": "^13.0.0",
    "web-push": "^3.6.7",
    "winston": "^3.9.0",
    "winston-daily-rotate-file": "^4.7.1",
    "zod": "^3.21.4"
  },
  "prisma": {
    "seed": "node src/prisma/seed.js"
  }
}
